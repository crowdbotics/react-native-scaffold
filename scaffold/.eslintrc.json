{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "no-debugger": "warn",
      "no-underscore-dangle": ["error", { "allow": ["__REDUX_DEVTOOLS_EXTENSION_COMPOSE__"] }],
      "global-require": "off",
      "react/jsx-filename-extension": ["warn", { "extensions": [".js"] }]
    },
    "globals": {
      "__DEV__": true,
      "window": true,
      "it": true,
      "expect": true,
      "fetch": true
    }
  }
